{
  "name": "review",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "created": {
      "type": "date",
      "default": "$now"
    },
    "is_online": {
      "type": "boolean"
    },
    "need_textbook": {
      "type": "boolean"
    },
    "course_difficulty": {
      "type": "number"
    },
    "course_recommend": {
      "type": "number"
    },
    "prof_rate": {
      "type": "number"
    },
    "prof_recommend": {
      "type": "number"
    },
    "content": {
      "type": "string"
    },
    "mandatary": {
      "type": "boolean"
    },
    "grade_received": {
      "type": "string"
    },
    "selected_labels": {
      "type": [
        "string"
      ]
    },
    "thumbs_up": {
      "type": "number",
      "default": 0
    },
    "thumbs_down": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "professor": {
      "type": "belongsTo",
      "model": "professor",
      "foreignKey": "professor_id"
    },
    "course_name": {
      "type": "belongsTo",
      "model": "course_name",
      "foreignKey": "course_name_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "thumbsDown"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "report"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "thumbsUp"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "rateCourse"
    }
  ],
  "methods": {}
}