{
  "name": "static_course",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "year": {
      "type": "string"
    },
    "term": {
      "type": "string"
    },
    "course_code": {
      "type": "string"
    },
    "course_type": {
      "type": "string"
    },
    "professor_name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "professor": {
      "type": "belongsTo",
      "model": "professor",
      "foreignKey": "professor_id"
    },
    "course_name": {
      "type": "belongsTo",
      "model": "course_name",
      "foreignKey": "course_name_id"
    },
    "year_term": {
      "type": "belongsTo",
      "model": "year_term",
      "foreignKey": "year_term_id"
    },
    "grade": {
      "type": "hasOne",
      "model": "grade",
      "foreignKey": "static_course_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "getCourseDetails"
    }
  ],
  "methods": {}
}