      "model": "professor",
      "foreignKey": "department_id"
    },
    "course_names": {
      "type": "hasMany",
      "model": "course_name",
      "foreignKey": "department_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
