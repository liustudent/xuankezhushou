{
  "name": "school",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "nameAbbrev": {
      "type": "string",
      "required": true
    },
    "current_year": {
      "type": "string",
      "required": true
    },
    "current_term": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "terms": {
      "type": "hasMany",
      "model": "term",
      "foreignKey": "school_id"
    },
    "year_terms": {
      "type": "hasMany",
      "model": "year_term",
      "foreignKey": "school_id"
    },
    "departments": {
      "type": "hasMany",
      "model": "department",
      "foreignKey": "school_id"
    },
    "course_names": {
      "type": "hasMany",
      "model": "course_name",
      "foreignKey": "school_id"
    },
    "professors": {
      "type": "hasMany",
      "model": "professor",
      "foreignKey": "school_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "getAllDepts"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "getCourseNames"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "searchProfs"
    }
  ],
  "methods": {}
}