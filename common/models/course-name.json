{
  "name": "course_name",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "dept_name": {
      "type": "string",
      "required": true
    },
    "course_number": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "course_name_id",
      "through": "watch_course"
    },
    "professors": {
      "type": "hasMany",
      "model": "professor",
      "foreignKey": "course_name_id",
      "through": "review"
    },
    "crawl_courses": {
      "type": "hasMany",
      "model": "crawl_course",
      "foreignKey": "course_name_id"
    },
    "static_courses": {
      "type": "hasMany",
      "model": "static_course",
      "foreignKey": "course_name_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "getCourseSections"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "normal",
      "permission": "ALLOW",
      "property": "getHistoryGrades"
    }
  ],
  "methods": {}
}