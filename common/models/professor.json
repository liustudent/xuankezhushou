{
  "name": "professor",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "default": "test"
    },
    "rmp_id": {
      "type": "string"
    },
    "rating": {
      "type": "string"
    },
    "name_subs": {
      "type": [
        "string"
      ]
    }
  },
  "validations": [],
  "relations": {
    "crawl_courses": {
      "type": "hasMany",
      "model": "crawl_course",
      "foreignKey": "professor_id"
    },
    "static_courses": {
      "type": "hasMany",
      "model": "static_course",
      "foreignKey": "professor_id"
    },
    "course_names": {
      "type": "hasMany",
      "model": "course_name",
      "foreignKey": "professor_id",
      "through": "review"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
